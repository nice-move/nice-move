{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "strict": true,
    "target": "esnext",
    "erasableSyntaxOnly": true
  },
  "display": "Base"
}
